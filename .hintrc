{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-emphasis-position",
          "scroll-behavior",
          "backdrop-filter",
          "-ms-filter",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "text-decoration-skip-ink",
          "overflow-anchor",
          "user-select"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "css-prefix-order": "off"
  }
}